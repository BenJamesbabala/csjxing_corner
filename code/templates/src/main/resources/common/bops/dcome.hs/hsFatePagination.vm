<div class="pagination" id="pagination">
	#parse("/common/bops/dcome.hs/_pagination.vm")
	<form method="get" action="$searchUrl" id="form_page">
		<input  type="hidden" name="hsId" value="$!hsId" />
		<input  type="hidden" name="gmtDay" value="$!gmtDay" />
		<input  type="hidden" name="gmtWeek" value="$!gmtWeek" />
		<input type="hidden" name="gmtMonth" value="$!gmtMonth" />
		
		<div class="page-op">
    		<span class="total-page">共 $totalPages 页</span>到<input type="text" name="page" value="$!curPage">页
    		<input type="submit" value="确定">
    	</div>
    </form>
</div>