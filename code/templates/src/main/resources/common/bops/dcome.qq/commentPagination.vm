<div class="pagination" id="pagination">
	#parse("/common/bops/dcome.qq/_pagination.vm")
		
	<form method="get" action="$searchUrl" id="form_page">
		<input name="itemId" type="hidden" value="$!itemId" />
		<input name="userId" type="hidden" value="$!userId" />
		<input name="status" type="hidden" value="$!status" />
		<input name="keywords" type="hidden" value="$!keywords" />
		<input name="gmtCreateStart" type="hidden" value="$!gmtCreateStartFormat"  /> 
		<input name="gmtCreateEnd" type="hidden" value="$!gmtCreateEndFormat" />
		<div class="page-op">
    		<span class="total-page">共 $totalPages 页</span>到<input type="text" name="page" value="$!curPage">页
    		<input type="submit" value="确定">
    	</div>
    </form>
</div>	
					