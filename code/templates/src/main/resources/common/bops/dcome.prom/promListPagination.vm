<div class="pagination" id="pagination">
	#parse("/common/bops/_pagination.vm")
	<form method="get" action="$searchUrl" id="form_page">
		<input  type="hidden" name="status" value="$!status" />
		<input  type="hidden" name="name" value="$!name" />
		<input type="hidden" name="startDate" value="$!startDateFormat" />
		<input type="hidden" name="endDate" value="$!endDateFormat" />
		
		<div class="page-op">
    		<span class="total-page">共 $totalPages 页</span>到<input type="text" name="page" value="$!curPage">页
    		<input type="submit" value="确定">
    	</div>
    </form>
</div>